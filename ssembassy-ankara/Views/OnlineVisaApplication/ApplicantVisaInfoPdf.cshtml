@model ssembassy_ankara.Models.OnlineVisaApplication

@{
    Layout = null;
    ViewBag.Title = Model.GivenNames + " | PDF";
}

<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <!-- bootstrap -->
    @Styles.Render("~/css/dashboard")
    <link href="~/Content/dashboard/visa_print.css" rel="stylesheet" type="text/css" />
    <title>@ViewBag.Title</title>
</head>
<body>
<!-- General Info and Personal Info -->
<div class="page">
    <div class="row">
        <div class="col-sm-10 col-md-8 col-md-offset-2 col-sm-offset-1">
            <div class="card-panel">
                @Html.Partial("_VisaHeader")
                
                <!-- Passport Details to Spouse Info -->
                @Html.Partial("_InfoPage1")

                <!-- Passport Details to Spouse Info -->
                @Html.Partial("_InfoPage2")
                
                <!-- Next of Kin to Have you ever... -->
                @Html.Partial("_InfoPage3")
                
                <!-- References to Declaration -->
                @Html.Partial("_InfoPage4")
            </div>
        </div>
    </div>
</div>
@Scripts.Render("~/bundles/jquery")
@Scripts.Render("~/bundles/bootstrapjs")
</body>
</html>
