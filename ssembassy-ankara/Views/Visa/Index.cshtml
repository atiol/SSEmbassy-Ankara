@model ssembassy_ankara.Models.VisaInfo

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="card-panel grey lighten-5 z-depth-0">
    <h5>VISA</h5>
    <div class="divider"></div>
    @if (Model != null)
    {
        <small class="text-gray">@Model.DateCreated.ToString("d")</small>
        @Html.Raw(Model.InfoEn)
    }
    else
    {
        <small class="grey-text lighten-5">No content available at the moment. Try again later</small>
    }
</div>